# Copy to `.env` (do NOT commit your real `.env`)

# ── Alpaca (used by backend-spawned MCP server) ──
ALPACA_API_KEY=
ALPACA_SECRET_KEY=
ALPACA_PAPER_TRADE=True

# ── LLM keys (protect these; backend rate limits LLM endpoints) ──
DEDALUS_API_KEY=
GEMINI_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
LLM_MODEL=gpt-4o

# ── News APIs (optional) ──
FINNHUB_API_KEY=
MARKETAUX_API_KEY=

# ── Voice (disabled in docker-compose by default) ──
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=
VOICE_ENABLED=false

